<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RatingBar">
        <attr name="number" format="integer" />
        <attr name="space" format="dimension" />
        <attr name="empty" format="reference" />
        <attr name="progress" format="reference" />
        <attr name="secondaryProgress" format="reference" />
    </declare-styleable>

    <declare-styleable name="GifView">
        <attr name="gif" format="reference" />
        <attr name="paused" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CustomTheme">
        <attr name="gifViewStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="WithoutPaddingTextView">
        <attr name="fontName" format="string" />
    </declare-styleable>

    <declare-styleable name="RoundProgressBar">
        <attr name="roundColor" format="color" />
        <attr name="roundProgressColor" format="color" />
        <attr name="roundWidth" format="dimension"></attr>
        <attr name="textIsDisplayable" format="boolean"></attr>
        <attr name="roundTextColor" format="color" />
        <attr name="roundTextSize" format="dimension" />
        <attr name="max" format="integer"></attr>
        <attr name="startAngle" format="integer"></attr>
        <attr name="roundStyle">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
    </declare-styleable>
    <declare-styleable name="LoadingProgressView">
        <attr name="progressColor" format="color" />
        <attr name="strokeWidth" format="dimension" />
    </declare-styleable>


    <declare-styleable name="MicView">
        <attr name="mic_wave_gap" format="dimension" />
        <attr name="mic_max_radius" format="dimension" />
        <attr name="ear_radius" format="dimension" />
        <attr name="mic_min_radius" format="dimension" />
        <attr name="mic_circle_width" format="dimension" />
        <attr name="mic_width" format="dimension" />
        <attr name="mic_static_width" format="dimension" />

        <attr name="mic_wave_width" format="dimension" />
        <attr name="mic_first_wave_radius" format="dimension" />
        <attr name="mic_left_ear_degree" format="dimension" />
        <attr name="mic_right_ear_degree" format="dimension" />

    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>


</resources>